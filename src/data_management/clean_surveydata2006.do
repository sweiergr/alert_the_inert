replace no_sw_cheaper=1 if lw20b=="Eerder van toepassing" | lw20b=="Helemaal van toepassing"
replace no_sw_cheaper=0 if lw20b=="Helemaal NIET van toepassing" | lw20b=="NIET echt van toepassing" | lw20b=="Weet niet (INT: niet voorlezen)"
