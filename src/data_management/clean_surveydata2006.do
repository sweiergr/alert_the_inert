//consumption
gen cons_day =ei7ad
replace cons_day="" if ei7ad=="Weet niet" | ei7ad=="9999999" | ei7ad=="999999999"
destring cons_day, replace
*tab cons_day

gen cons_night =ei7ae
replace cons_night="" if ei7ae=="Weet niet" 
replace cons_night="1057" if ei7ae=="1057999999" 
replace cons_night="505" if ei7ae=="5059999999"
destring cons_night, replace

gen cons_peak =ei7af
replace cons_peak="" if ei7af=="Weet niet" 
destring cons_peak, replace

*SUM
replace cons_day=0 if cons_day==.
replace cons_night=0 if cons_night==.
replace cons_peak=0 if cons_peak==.
gen consumption_help=cons_day + cons_night + cons_peak

gen consumption=.
replace consumption=1 if consumption_help<900
replace consumption=2 if consumption_help>=900 & consumption_help<2350
replace consumption=3 if consumption_help>=2350 & consumption_help<5500
replace consumption=4 if consumption_help>=5500 & consumption_help<13750
replace consumption=5 if consumption_help>=13750
replace consumption=6 if ei7ad=="Weet niet" | ei7ad=="9999999" | ei7ad=="999999999"
label var consumption "yearly electricity consumption (past year)"
#delimit;
label define consumptionl 1 "<900kWh" 2 "900-2350 kWh" 3 "2350-5500 kWh" 4 "5500-13750 kWh"
	5 ">13750 kWh" 6 "don't know"
;
#delimit cr
label values consumption consumptionl
*check 
tab consumption 
drop consumption_help

