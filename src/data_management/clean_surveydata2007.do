gen no_sw_wrong=.
replace no_sw_wrong=1 if LW20F=="Eerder van toepassing" | LW20F=="Helemaal van toepassing"
replace no_sw_wrong=0 if LW20F=="Helemaal NIET van toepassing" | LW20F=="NIET echt van toepassing" | LW20F=="Weet niet (INT: niet voorlezen)"
//TODO: don't know
label var no_sw_wrong "Haven't switched supplier bec too many things could go wrong"
label define no_sw_wrongl 0 "Not true" 1 "True"
label values no_sw_wrong no_sw_wrongl
*check
tab LW20F
tab no_sw_wrong

gen consumption=.
destring EI5A, replace
replace consumption=1 if EI5A<900
replace consumption=2 if EI5A>=900 & EI5A<2350
replace consumption=3 if EI5A>=2350 & EI5A<5500
replace consumption=4 if EI5A>=5500 & EI5A<13750
replace consumption=5 if EI5A>=13750
replace consumption=6 if BVEI5=="Weet niet"
label var consumption "yearly electricity consumption (past year)"
#delimit;
label define consumptionl 1 "<900kWh" 2 "900-2350 kWh" 3 "2350-5500 kWh" 4 "5500-13750 kWh"
	5 ">13750 kWh" 6 "don't know"
;
#delimit cr
label values consumption consumptionl
*check 
tab consumption 

gen energycost=.
label var energycost "How important are energy costs within total HH costs"
replace energycost=1 if EI8=="Helemaal niet belangrijk"
replace energycost=2 if EI8=="Niet echt zo belangrijk"
replace energycost=3 if EI8=="Nogal belangrijk"
replace energycost=4 if EI8=="Zeer belangrijk"
replace energycost=5 if EI8=="Geen mening  (INT: niet voorlezen)"
label define energycostl 1 "not important at all" 2 "rather not important" 3 "rather important" 4 "very important" 5 "no opinion"
label values energycost energycostl
*check
tab energycost

gen contacted=.
replace contacted=1 if LW1BS=="Ja"
replace contacted=2 if LW1BS=="Neen"
replace contacted=3 if LW1BS=="Weet niet"
label var contacted "Have you been contacted by supplier in last 12 months"
